   "source": [
    "t,r,theta,phi = pt.coords('t,r,theta,phi')\n",
    "kappa = pt.con('kappa')\n",
    "a =pt.fun('a','t')\n",
    "ds = 'ds2 =-dt**2 +a**2*(dr**2 + kappa*(r**2*dtheta**2 + r**2*sin(theta)**2 * dphi**2))'\n",
    "gt = pt.metric(ds)\n",
    "G = pt.einstein()\n",
    "G.display(aslist=True)"
   ]
